FAL_KEY=your_fal_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
REDIS_URL=redis://localhost:6379/0

# Optional overrides
# Gemini Image Generation - Workflow-based model selection
# - CREATE workflow uses Pro model (with thinking)
# - EDIT workflow uses Flash model (faster, no thinking)
# GEMINI_PRO_MODEL=gemini-3-pro-image-preview
# GEMINI_FLASH_MODEL=gemini-2.5-flash-image
# GEMINI_THINKING_LEVEL=low  # Applied to Pro model only (CREATE workflow)
# GEMINI_IMAGE_SIZE=1K  # Image resolution (1K, 2K, 4K for Pro)
# GEMINI_IMAGE_ASPECT_RATIO=1:1

# Artifact Storage (Demo/Test Mode)
# SAVE_ARTIFACTS_LOCALLY=false  # Set to true to save all artifacts to backend/tests/artifacts/
#                                # Saves: Gemini images, GLB models, videos, no-bg images, state.json
#                                # Useful for demos, testing, and debugging
